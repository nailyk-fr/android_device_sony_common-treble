/data/etc/wlan.*                                                u:object_r:wlan_data_file:s0
/data/etc/bluetooth.*                                           u:object_r:bluetooth_data_file:s0
/data/fpc(/.*)?                                                 u:object_r:fpc_data_file:s0
/data/fpc/oem                                                   u:object_r:fpc_socket:s0
/data/crashdata(/.*)?                                           u:object_r:crash_data_file:s0

